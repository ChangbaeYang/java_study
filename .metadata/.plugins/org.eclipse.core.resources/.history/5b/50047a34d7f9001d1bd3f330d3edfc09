package generic;

class Soock {
	private String name;
	
	public void setSoock(String name) {
		this.name = name;
	}
	
	public Soock(String name) {
		this.name = name;
	}
}

class Ssal {
	private String name;
	
	public void setSsal(String name) {
		this.name = name;
	}
	
	public Ssal(String name) {
		this.name = name;
	}
}



class RiceCake {
	private Object material;
	
	public void setMaterial(Object material) {
		this.material = material;
	}
	
	public Object getMaterial() {
		return material;
	}
}




public class WhyGeneric {

	public static void main(String[] args) {
		Soock soock = new Soock("ì‘¥");
		RiceCake riceCake1 = new RiceCake();
		riceCake1.setMaterial(soock);
		
		
	}

}
