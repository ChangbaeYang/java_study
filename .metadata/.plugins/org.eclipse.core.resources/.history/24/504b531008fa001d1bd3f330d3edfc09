package generic;

public class Ssal extends Material{
	private String name;
	
	public void setSsal(String name) {
		this.name = name;
	}
	
	public Ssal(String name) {
		this.name = name;
	}
	
	@Override
	public String toString() {
		return this.name;
	}

	@Override
	public void taste() {
		System.out.println("단맛!");
	}
}

